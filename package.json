{
  "name": "osmcha-frontend",
  "version": "0.8.0",
  "engines": {
    "node": ">=7.0"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "changeset-map": "1.0.8",
    "history": "^4.6.2",
    "immutable": "^3.8.1",
    "lodash.debounce": "^4.0.8",
    "mousetrap": "^1.6.1",
    "raven-js": "^3.16.0",
    "react": "^15.6.1",
    "react-anchorify-text": "^2.1.0",
    "react-click-outside": "^2.3.1",
    "react-dom": "^15.6.1",
    "react-ga": "^2.2.0",
    "react-notification-system": "^0.2.14",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "react-select": "^1.0.0-rc.5",
    "react-transition-group": "^1.2.0",
    "redux": "^3.7.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.3",
    "showdown": "^1.7.1",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "check-node-version": "^2.0.1",
    "flow-bin": "^0.48.0",
    "gh-pages": "^1.0.0",
    "husky": "^0.13.4",
    "jest-cli": "^20.0.4",
    "lint-staged": "^3.6.1",
    "mockdate": "^2.0.1",
    "prettier": "^1.4.4",
    "prettier-eslint-cli": "^4.1.1",
    "react-scripts": "1.0.7",
    "react-test-renderer": "^15.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "why-did-you-update": "0.0.8"
  },
  "scripts": {
    "precommit": "lint-staged",
    "flow": "flow",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "flow && react-scripts test --env=jsdom",
    "prebuild:dev": "npm run homepage-dev",
    "build:dev": "REACT_APP_VERSION=\"$(node -e \"console.log(require('./package.json').version + ' Dev')\")\" REACT_APP_STACK=DEV react-scripts build",
    "deploy:dev": "gh-pages -d build",
    "prebuild:staging": "npm run homepage-staging",
    "build:staging": "REACT_APP_VERSION=\"$(node -e \"console.log(require('./package.json').version + ' Staging')\")\" REACT_APP_STACK=STAGING react-scripts build",
    "deploy:staging": "gh-pages -d build -b oh-pages",
    "prebuild:prod": "npm run homepage-prod",
    "build:prod": "REACT_APP_VERSION=\"$(node -e \"console.log(require('./package.json').version)\")\" REACT_APP_STACK=PRODUCTION react-scripts build",
    "deploy:prod": "gh-pages -d build -b oh-pages",
    "homepage-dev": "REACT_APP_HOMEPAGE=https://mapbox.github.io/osmcha-frontend node scripts/fix_homepage.js",
    "homepage-staging": "REACT_APP_HOMEPAGE=https://osmcha-django-staging.tilestream.net node scripts/fix_homepage.js",
    "homepage-prod": "REACT_APP_HOMEPAGE=https://osmcha.mapbox.com node scripts/fix_homepage.js"
  },
  "homepage": "https://mapbox.github.io/osmcha-frontend",
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote",
      "git add"
    ]
  }
}
